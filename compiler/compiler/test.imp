PROGRAM IS
    a, b, div, mod, tab[-5:5]
BEGIN
    READ a;       # Pobierz liczbę do podzielenia
    READ b;       # Pobierz dzielnik
   
        div := a / b;  # Oblicz iloraz
        mod := a % b;  # Oblicz resztę
    
    WRITE div;    # Wypisz wynik dzielenia
    WRITE mod;    # Wypisz resztę z dzielenia

    tab[2] := div;  # Zapisz wynik dzielenia do tablicy
    tab[3] := mod;  # Zapisz wynik modulo do tablicy
    
    WRITE tab[2];  # Wypisz wartość z tablicy
    WRITE tab[3];  # Wypisz wartość z tablicy
END
