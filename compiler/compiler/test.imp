PROGRAM IS
x, sum, tab[1:5],p
BEGIN
    # Inicjalizacja wartości tablicy
    p:=2;
    READ tab[p];
    tab[3] := 30;
    tab[4] := 40;
    tab[5] := 50;

    # Inicjalizacja zmiennych
    x := 1; # Indeks tablicy
    sum := 0; # Suma wartości
    tab[x] := 5;
    # Pętla WHILE: Sumowanie wartości tablicy
    WHILE x <= tab[x] DO
        sum := sum + tab[x];
        
        x := x + 1; # Przejście do następnego indeksu
    ENDWHILE

    # Wypisanie sumy wartości w tablicy
    WRITE sum; # Powinno wypisać 150

    # Resetowanie zmiennej x
    x := 5;

    # Pętla WHILE: Wypisywanie tablicy od końca
    WHILE x >= 1 DO
        WRITE tab[x]; # Wypisanie wartości tablicy
        x := x - 1; # Przejście do poprzedniego indeksu
    ENDWHILE
END
